-- Scripts:

local function CTKH_fake_script() -- InfiniteYield1.InfYieldLS1 
	local script = Instance.new('LocalScript', InfiniteYield1)

	script.parent.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
	end)
	
	
end
coroutine.wrap(CTKH_fake_script)()
local function SZEY_fake_script() -- GrenadeNuke.GrenadeNukeLS 
	local script = Instance.new('LocalScript', GrenadeNuke)

	script.parent.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet('https://raw.githubusercontent.com/22kyasue/Fe-Script/master/Grenade'))()
	end)
	
	
end
coroutine.wrap(SZEY_fake_script)()
local function CFZDEI_fake_script() -- Swagmode.SwagmodeLS 
	local script = Instance.new('LocalScript', Swagmode)

	script.parent.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet('https://raw.githubusercontent.com/lerkermer/lua-projects/master/SwagModeV002'))()
	end)
	
	
end
coroutine.wrap(CFZDEI_fake_script)()
local function FTMKCYW_fake_script() -- Main.GameDetermination 
	local script = Instance.new('LocalScript', Main)

	local Info = TweenInfo.new(1)
	-------------------------------------------------------------------
	--Dahood
	local DahoodUI = script.parent.Background.DahoodUI
	local DecorationFrame1 = script.parent.Background.DahoodUI.DecorationFrame1
	local DahoodLogo = script.parent.Background.DahoodUI.DaHoodLogo
	local ScriptScroll1 = script.Parent.Background.DahoodUI.ScriptScroll1
	local GrenadeNuke = script.Parent.Background.DahoodUI.ScriptScroll1.GrenadeNuke
	local Swagmode = script.Parent.Background.DahoodUI.ScriptScroll1.Swagmode
	local InfiniteYield1 = script.Parent.Background.DahoodUI.ScriptScroll1.InfiniteYield1
	local DahoodLabel1 = script.Parent.Background.DahoodUI.ScriptScroll1.DahoodLabel1
	-------------------------------------------------------------------
	--ErrorScreen
	local Error1 = script.parent.Background.Error1
	local DecorationFrame2 = script.parent.Background.Error1.DecorationFrame2
	local ErrorImage = script.parent.Background.Error1.ErrorImage
	local ErrorLabel = script.parent.Background.Error1.ErrorLabel
	local ErrorMessage = script.parent.Background.Error1.ErrorMessage
	-------------------------------------------------------------------
	--Dahood
	wait(5.40)	
	if game.PlaceId ==2788229376 then
	DahoodUI:TweenSize(UDim2.new(0,200,0,368))
	DahoodUI:TweenPosition(UDim2.new(0.5,0,0.53,0))	
	local Tween5 = game:GetService("TweenService"):Create(DahoodUI,Info,{BackgroundTransparency=.2})
	local Tween6 = game:GetService("TweenService"):Create(DecorationFrame1,Info,{BackgroundTransparency=0})
	local Tween7 = game:GetService("TweenService"):Create(DahoodLogo,Info,{ImageTransparency=0})
	local Tween8 = game:GetService("TweenService"):Create(ScriptScroll1,Info,{ScrollBarThickness=5})
	local Tween9 = game:GetService("TweenService"):Create(GrenadeNuke,Info,{BackgroundTransparency=0})
	local Tween10 = game:GetService("TweenService"):Create(GrenadeNuke,Info,{TextTransparency=0})
	local Tween11 = game:GetService("TweenService"):Create(InfiniteYield1,Info,{BackgroundTransparency=0})
	local Tween12 = game:GetService("TweenService"):Create(InfiniteYield1,Info,{TextTransparency=0})
	local Tween13 = game:GetService("TweenService"):Create(Swagmode,Info,{BackgroundTransparency=0})
	local Tween14 = game:GetService("TweenService"):Create(Swagmode,Info,{TextTransparency=0})
	local Tween15 = game:GetService("TweenService"):Create(DahoodLabel1,Info,{TextTransparency=0})
	
	
	wait(5)
	Tween5:Play()
	Tween6:Play()
	Tween7:Play()
	Tween8:Play()
	wait(.3)
	Tween9:Play()
	Tween10:Play()
	Tween11:Play()
	Tween12:Play()
	Tween13:Play()
	Tween14:Play()
	Tween14:Play()
	-------------------------------------------------------------------------
	
	else
	Error1:TweenSize(UDim2.new(0,200,0,368))
	Error1:TweenPosition(UDim2.new(0.5,0,0.53,0))
	local Tween16 = game:GetService("TweenService"):Create(Error1,Info,{BackgroundTransparency=.2})
	local Tween17 = game:GetService("TweenService"):Create(DecorationFrame2,Info,{BackgroundTransparency=0})
	local Tween18 = game:GetService("TweenService"):Create(ErrorImage,Info,{ImageTransparency=0})
	local Tween19 = game:GetService("TweenService"):Create(ErrorLabel,Info,{TextTransparency=0})
	local Tween20 = game:GetService("TweenService"):Create(ErrorMessage,Info,{TextTransparency=0})
	wait(1.5)
	Tween16:Play()
	Tween17:Play()
	Tween18:Play()
	wait(.3)
	Tween19:Play()
	Tween20:Play()
		
	
	end
	
	
end
coroutine.wrap(FTMKCYW_fake_script)()
local function FSTU_fake_script() -- Main.BootScreen 
	local script = Instance.new('LocalScript', Main)

	local Info = TweenInfo.new(1)
	local Highbar = script.parent.NetworkBars.HighBar
	local NormalBar = script.Parent.NetworkBars.NormalBar
	local LowBar = script.Parent.NetworkBars.LowBar
	local Background = script.parent.Background
	--------------------------------------------------------------------------------------------------
	local Tween1 = game:GetService("TweenService"):Create(Background,Info,{ImageTransparency=0})
	local Tween2 = game:GetService("TweenService"):Create(LowBar,Info,{BackgroundTransparency=0})
	local Tween3 = game:GetService("TweenService"):Create(NormalBar,Info,{BackgroundTransparency=0})
	local Tween4 = game:GetService("TweenService"):Create(Highbar,Info,{BackgroundTransparency=0})
	--------------------------------------------------------------------------------------------------
	
	--------------------------------------------------------------------------------------------------
	wait(4.5)
	Tween1:Play()
	wait(.25)
	Tween2:Play()
	wait(.3)
	Tween3:Play()
	wait(.35)
	Tween4:Play()
end
coroutine.wrap(FSTU_fake_script)()
local function RUOR_fake_script() -- Main.BootChimePlay 
	local script = Instance.new('LocalScript', Main)

	local Boot = script.parent.BootChime
	------------------------------------------------------
	wait(3.2)
		Boot:Play()
	
	
end
coroutine.wrap(RUOR_fake_script)()
local function DODX_fake_script() -- Main.DHVisibility 
	local script = Instance.new('LocalScript', Main)

	if game.PlaceId ==2788229376 then
		script.parent.Background.DahoodUI.Visible = true
	else
		script.parent.Background.Dahoodui.Visible = false 
	end
end
coroutine.wrap(DODX_fake_script)()
local function TGNS_fake_script() -- Main.Keybind 
	local script = Instance.new('LocalScript', Main)

	local open = true
	local keystatus
	local mouse = game.Players.LocalPlayer:GetMouse()
	keystatus=mouse.KeyDown:connect(function(key)
		if key == "z" then
			if script.Parent.Visible == true then
				script.Parent.Visible = false
			else
				script.Parent.Visible = true
			end
		end
	end)
end
coroutine.wrap(TGNS_fake_script)()
